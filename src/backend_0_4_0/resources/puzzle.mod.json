{
  "formatVersion": 2,
  "name": "Cosmic API",
  "id": "cosmic-api",
  "version": "${version}",
  "description": "This is the cosmic-api module of the Puzzle Loader.",
  "authors": [ "Mr_Zombii" ],
  "meta": {
    "icon": "cosmic-api:icons/icon.png"
  },
  "entrypoints": {
    "postInit": [
      "io.github.puzzle.cosmic.CosmicServerAPI"
    ],
    "client_postInit": [
      "io.github.puzzle.cosmic.CosmicAPI"
    ]
  },
  "depends": {
    "cosmic-reach": "${crVersion}",
    "puzzle-loader": ">=2.0.0"
  },
  "allowedSides": {
    "client": false,
    "server": false
  },
  "mixins": [
    "puzzle_cosmic_api.mixins.json"
  ]
}